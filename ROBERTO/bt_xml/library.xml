<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <ReactiveSequence>
            <Fallback>
                <Action ID="Search" waypoint="{wp}"/>
                <Action ID="ReadQR" waypoint="{wp]"/>
            </Fallback>
            <Sequence>
                <Action ID="NavObjective" waypoint="{wp}"/>
                <ReactiveFallback>
                    <Action ID="StoreObject"/>
                </ReactiveFallback>
            </Sequence>
            <Action ID="NavOdom" odom=""/>
        </ReactiveSequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Undefined ID=""/>
        <Action ID="NavObjective">
            <input_port name="waypoint"/>
        </Action>
        <Action ID="NavOdom">
            <input_port name="odom"/>
        </Action>
        <Action ID="ReadQR">
            <output_port name="waypoint"/>
        </Action>
        <Action ID="Search">
            <output_port name="waypoint"/>
        </Action>
        <Action ID="StoreObject"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
